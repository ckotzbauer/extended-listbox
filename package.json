{
  "name": "extended-listbox",
  "version": "5.0.2",
  "description": "A simple to use javascript component as powerful alternative to the HTML <select> tag",
  "main": "dist/js/extended-listbox.js",
  "typings": "dist/extended-listbox.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckotzbauer/extended-listbox.git"
  },
  "keywords": [
    "listbox",
    "select",
    "multiselect",
    "form",
    "input",
    "ui"
  ],
  "author": "Christian Kotzbauer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ckotzbauer/extended-listbox/issues"
  },
  "homepage": "https://github.com/ckotzbauer/extended-listbox#readme",
  "files": [
    "dist",
    "doc/pages",
    "doc/CHANGELOG.md",
    "doc/ROADMAP.md"
  ],
  "scripts": {
    "clean": "node node_modules/rimraf/bin.js build/out build/coverage",
    "compile:dev": "node node_modules/webpack/bin/webpack.js",
    "compile:prod": "node node_modules/webpack/bin/webpack.js -p",
    "compile": "npm run clean && npm run compile:dev && npm run compile:prod",
    "test": "npm run compile && jest --config jest.json",
    "lint": "node node_modules/tslint/bin/tslint -c tslint.json --project tsconfig.json -t verbose -o build/out/tslint.txt",
    "dist": "npm run compile && node build/scripts/prepublish.js"
  },
  "devDependencies": {
    "@types/jest": "~26.0.13",
    "awesome-typescript-loader": "5.2.1",
    "css-loader": "4.3.0",
    "globby": "11.0.1",
    "jest": "~26.4.2",
    "mini-css-extract-plugin": "0.11.1",
    "node-sass": "4.14.1",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.2",
    "ts-jest": "~26.3.0",
    "tslint": "6.1.3",
    "typescript": "4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "yargs": "16.0.3"
  },
  "dependencies": {}
}
